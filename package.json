{
  "name": "front-pressed-lambda",
  "description": "Record last pressed times of fronts",
  "main": "lambda/index.js",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "cloudformation": false,
  "buildDir": "tmp/lambda",
  "isAwsLambda": true,
  "riffraffFile": "./riff-raff.yaml",
  "devDependencies": {
    "ava": "^0.15.2",
    "aws-sdk": "^2.4.13",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-plugin-transform-es2015-function-name": "^6.9.0",
    "babel-plugin-transform-es2015-parameters": "^6.11.4",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.8.0",
    "babel-plugin-transform-es2015-spread": "^6.8.0",
    "eslint": "^3.2.2",
    "eslint-config-prettier": "^6.7.0",
    "exec-chainable": "0.0.3",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-yaml": "^1.0.1",
    "node-riffraff-artefact": "^2.0.1",
    "rollup": "^0.34.3",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^3.3.1",
    "rollup-plugin-json": "^2.0.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "sinon": "^4.3.0"
  },
  "scripts": {
    "lint": "eslint lambda/*.js test/*.js",
    "test": "npm run bundle && ava",
    "start": "node lambda/index.js",
    "rollup": "rollup -c",
    "secrets": "./bin/get-secrets.sh $FRONT_PRESSED_LAMBDA_BUCKET > tmp/config.json",
    "bundle": "mkdir -p tmp && npm run secrets && npm run rollup",
    "deploy": "gulp deploy && VERBOSE=true ARTEFACT_PATH=$PWD riffraff-artefact"
  },
  "dependencies": {
    "async-es": "^2.0.1",
    "simple-get-promise": "^1.0.2"
  }
}

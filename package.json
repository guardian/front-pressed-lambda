{
  "name": "front-pressed-lambda",
  "description": "Record last pressed times of fronts",
  "main": "lambda/index.js",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "devDependencies": {
    "ava": "^0.14.0",
    "aws-sdk": "^2.3.3",
    "babel-preset-es2015-rollup": "^1.1.1",
    "eslint": "^2.7.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-yaml": "^1.0.1",
    "gulp-zip": "^3.2.0",
    "node-riffraff-artefact": "^1.6.5",
    "rollup": "^0.25.8",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-json": "^2.0.0",
    "rollup-plugin-node-resolve": "^1.5.0"
  },
  "scripts": {
    "lint": "eslint lambda/*.js test/*.js",
    "test": "npm run lint && npm run bundle && ava",
    "start": "node lambda/index.js",
    "rollup": "rollup -c",
    "secrets": "./bin/get-secrets.sh $FRONT_PRESSED_LAMBDA_BUCKET > tmp/config.json",
    "bundle": "mkdir -p tmp && npm run secrets && npm run rollup",
    "package": "npm run bundle && gulp package",
    "deploy": "npm run package && gulp deploy"
  },
  "dependencies": {
    "async-es": "^2.0.0-rc.2"
  }
}
